{
  "version": 2,
  "alias": "thoughtful-client",
  "name": "thoughtful-client",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
     "src": "^/favicon.ico$",
     "dest": "/favicon.ico"
    },
    {
     "src": "^/manifest.json$",
     "dest": "/manifest.json"
    },
    { 
      "handle": "filesystem"
    },
    { 
      "src": "/.*", "dest": "/index.html" 
    }
  ]
}